{
  "name": "meu-backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node src/index.ts",
  "start:dev": "npx prisma generate && nodemon --exec ts-node src/index.ts",
    "build": "npm run prisma:generate && tsc",
